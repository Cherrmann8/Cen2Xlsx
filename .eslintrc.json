{
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "jest": true
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "sourceType": "module"
    },
    "rules": {
      "no-const-assign": "warn",
      "no-this-before-super": "warn",
      "no-undef": "warn",
      "no-unreachable": "warn",
      "no-unused-vars": "warn",
      "constructor-super": "warn",
      "valid-typeof": "warn",
      "quotes": [
        2,
        "double"
      ],
      "semi": [
        "warn"
      ],
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "no-console": "off",
      "no-lonely-if": "off",
      "no-loop-func": "off",
      "object-shorthand": "off",
      "prefer-const": "warn",
      "react/jsx-boolean-value": "warn",
      "no-unneeded-ternary": "warn",
      "global-require": "warn",
      "comma-dangle": "warn",
      "object-curly-newline": "warn",
      "operator-linebreak": "warn",
      "linebreak-style": "off"

    },
    "parser": "babel-eslint",
    "extends": "airbnb",
    "plugins": [
      "react",
      "jsx-a11y",
      "import"
    ]
  }